    "query <- \"select YEAR(YEAR),GEO,CROP_TYPE, SEED_AREA, HARVESTED_AREA,100*(SEED_AREA-HARVESTED_AREA)/SEED_AREA as UNHARVESTED_AREA from CROP where year(year)>=2000 and GEO='Alberta' and CROP_TYPE='Barley';\"\n",
    "view <- sqlQuery(conn,query)\n",
